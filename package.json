{
  "name": "react-avancado-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql  schema-temp.gql",
    "schema:convert": "node convertToNonNull.js",
    "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated",
    "types:generate": "yarn schema:download && yarn schema:convert && yarn codegen && echo \"\n\n**** ðŸŽ‰ðŸŽ‰ðŸŽ‰ GRAPHQL TYPES CREATED ðŸŽ‰ðŸŽ‰ðŸŽ‰ ****\n\"",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "test": "jest --maxWorkers=50%",
    "test:watch": "jest --watch --maxWorkers=25%",
    "test:ci": "jest --runInBand",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "postinstall": "husky install",
    "prepare": "husky install",
    "typecheck": "tsc --project tsconfig.json --incremental false --noEmit"
  },
  "resolutions": {
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --passWithNoTests --bail"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@types/lodash.xor": "^4.5.7",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest-environment-jsdom": "^28.1.3",
    "lodash.xor": "^4.5.0",
    "next": "^12.2.2",
    "next-pwa": "^5.5.4",
    "polished": "^4.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-slick": "^0.29.0",
    "styled-components": "5.3.5",
    "styled-icons": "^10.45.0",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "6.5.9",
    "@storybook/builder-webpack5": "^6.5.8",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "6.5.9",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.3.0",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.6",
    "@types/react": "^18.0.15",
    "@types/react-slick": "^0.23.10",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.7",
    "apollo": "^2.34.0",
    "eslint": "^8.20.0",
    "eslint-config-next": "^12.2.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-styled-components": "^7.0.5",
    "lint-staged": "^13.0.3",
